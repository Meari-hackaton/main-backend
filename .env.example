# Application
APP_NAME="Meari Backend"
DEBUG=True

# Database
DATABASE_URL=postgresql+asyncpg://meari_user:meari_password@localhost/meari_db

# Security
SECRET_KEY=your-secret-key-here

# AI/LLM APIs
GEMINI_API_KEY=your-gemini-api-key
GEMINI_API_KEY2=your-gemini-api-key-2
GEMINI_API_KEY3=your-gemini-api-key-3

# BigKinds API
BIGKINDS_ACCESS_KEY=your-bigkinds-key

# 청년정책 API
YOUTH_POLICY_API_KEY=your-youth-policy-key

# Milvus Vector Database
MILVUS_URI=your-milvus-uri
MILVUS_TOKEN=your-milvus-token

# Neo4j Graph Database
NEO4J_URI=your-neo4j-uri
NEO4J_USER=neo4j
NEO4J_PASSWORD=your-neo4j-password

# Embedding Model
EMBEDDING_MODEL_NAME=nlpai-lab/KURE-v1

# OAuth Configuration (Optional)
GOOGLE_CLIENT_ID=your-google-client-id
GOOGLE_CLIENT_SECRET=your-google-client-secret
GOOGLE_REDIRECT_URI=http://localhost:8000/auth/google/callback

KAKAO_CLIENT_ID=your-kakao-client-id
KAKAO_CLIENT_SECRET=your-kakao-client-secret
KAKAO_REDIRECT_URI=http://localhost:8000/auth/kakao/callback

# Domain
DOMAIN=https://mearihearo.com/
FRONTEND_URL=http://localhost:3000/

# Session Settings
COOKIE_SECURE=false
COOKIE_SAMESITE=lax
SESSION_COOKIE_NAME=meari_session
SESSION_EXPIRES_DAYS=7